{
    "schemes": [
        "http"
    ],
    "swagger": "2.0",
    "info": {
        "description": "Backend API for floorplan processing and detection",
        "title": "FloorPlan Whiteboard API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
            "url": "http://www.swagger.io/support",
            "email": "support@swagger.io"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/api/v1/process/edges": {
            "post": {
                "description": "Process a floorplan image to detect edges using Canny edge detection",
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "edge-detection"
                ],
                "summary": "Detect edges in a floorplan image",
                "operationId": "detectEdges",
                "parameters": [
                    {
                        "type": "file",
                        "description": "Floorplan image file",
                        "name": "file",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "type": "number",
                        "default": 1.2,
                        "description": "Blur radius for preprocessing",
                        "name": "blur_radius",
                        "in": "formData"
                    },
                    {
                        "type": "number",
                        "default": 50,
                        "description": "Canny low threshold",
                        "name": "canny_low",
                        "in": "formData"
                    },
                    {
                        "type": "number",
                        "default": 150,
                        "description": "Canny high threshold",
                        "name": "canny_high",
                        "in": "formData"
                    },
                    {
                        "type": "integer",
                        "default": 800,
                        "description": "Maximum width for resizing",
                        "name": "resize_max_width",
                        "in": "formData"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Edge detection result",
                        "schema": {
                            "$ref": "#/definitions/handler.EdgeDetectionResponse"
                        }
                    },
                    "400": {
                        "description": "Bad request",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/process/edges-json": {
            "post": {
                "description": "Process a floorplan image (provided as base64) to detect edges",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "edge-detection"
                ],
                "summary": "Detect edges using JSON request with base64 image",
                "operationId": "detectEdgesJSON",
                "parameters": [
                    {
                        "description": "JSON request with image and options",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Edge detection result with processed image",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "400": {
                        "description": "Bad request",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/upload": {
            "post": {
                "description": "Upload a floorplan image (PNG, JPG, JPEG) and return the detected rooms",
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "upload"
                ],
                "summary": "Upload a floorplan image",
                "operationId": "uploadFloorplan",
                "parameters": [
                    {
                        "type": "file",
                        "description": "Floorplan image file",
                        "name": "file",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Detection results with rooms",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "400": {
                        "description": "Bad request",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "handler.EdgeDetectionResponse": {
            "type": "object",
            "properties": {
                "message": {
                    "type": "string"
                },
                "processed_image": {
                    "description": "data:image/png;base64,...",
                    "type": "string"
                }
            }
        }
    }
}